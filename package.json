  "scripts": {
    "build:affected": "npx nx affected -t build",
    "build:affected:dev": "npx nx affected -t build --configuration=development",
    "build:all": "npx nx build duality-social-lib && npx nx build duality-social-node && npx nx build duality-social-react",
    "build:all:dev": "npx nx build duality-social-lib --configuration=development && npx nx build duality-social-node --configuration=development && npx nx build duality-social-react --configuration=development",
    "build:lib": "npx nx build duality-social-lib",
    "build:node": "npx nx build duality-social-node",
    "build:node:dev": "npx nx build duality-social-node --configuration=development",
    "build:react": "npx nx build duality-social-react",
    "build:react:dev": "npx nx build duality-social-react --configuration=development",
    "clean": "rm -rf dist",
    "docker:dev": "docker-compose -f .devcontainer/docker-compose.yml",
    "docker:dev:ps": "docker-compose -f .devcontainer/docker-compose.yml ps",
    "newSecret": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\"",
    "npm-globals:install": "npm install -g npm npx nx yarn",
    "vscode:create-settings": "/bin/bash ./.devcontainer/create-settings.sh",
    "serve:node": "npx nx serve duality-social-node",
    "serve:react": "npx nx serve duality-social-react"
  },
