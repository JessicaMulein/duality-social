{
  "name": "duality-social",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "fontawesome:setup": "scripts/setup-fa-npm.sh",
    "newSecret": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\"",
    "all": "yarn && cd libs/duality-social-lib && yarn && cd ../../apps/duality-social-angular && yarn && cd ../../apps/duality-social-node && yarn && cd ../..",
    "all:upgrade": "yarn upgrade && cd libs/duality-social-lib && yarn upgrade && cd ../../apps/duality-social-angular && yarn upgrade && cd ../duality-social-node && yarn upgrade && cd ../..",
    "all:build": "npx nx build duality-social-lib && npx nx build duality-social-angular && npx nx build duality-social-node",
    "all:build:verbose": "npx nx build duality-social-lib --verbose && npx nx build duality-social-angular --verbose && npx nx build duality-social-node --verbose",
    "all:build:dev": "npx nx build duality-social-lib --configuration development && npx nx build duality-social-angular --configuration development && npx nx build duality-social-node --configuration development",
    "all:test": "npx nx test duality-social-lib && npx nx test duality-social-angular && npx nx test duality-social-node",
    "build": "yarn all:build",
    "serve": "npx nx serve duality-social-node",
    "serve:dev": "npx nx serve duality-social-node --configuration development",
    "npm-globals": "npm install -g yarn @angular/cli @nrwl/cli npx nx jest cmake-js realm-cli",
    "azure:login": "az login"
  },
  "private": true,
