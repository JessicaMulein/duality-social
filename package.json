{
  "name": "duality-social",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "secret:new": "node -e \"console.log (require('crypto').randomBytes(32).toString ('hex'));\"",
    "install:all": "yarn && cd libs/duality-social-lib && yarn && cd ../../apps/duality-social-angular && yarn && cd ../../apps/duality-social-node && yarn && cd ../..",
    "upgrade:all": "yarn upgrade && cd libs/duality-social-lib && yarn upgrade && cd ../../apps/duality-social-angular && yarn upgrade && cd ../duality-social-node && yarn upgrade && cd ../..",
    "build:angular": "npx nx build duality-social-angular --configuration production",
    "build:angular:dev": "npx nx build duality-social-angular --configuration development",
    "build:lib": "npx nx build duality-social-lib --configuration production",
    "build:lib:dev": "npx nx build duality-social-lib --configuration development",
    "build:affected": "npx nx affected:build --configuration production",
    "build:affected:dev": "npx nx affected:build --configuration development",
    "build:all": "npx nx build duality-social-lib --configuration production && npx nx build duality-social-angular --configuration production&& npx nx build duality-social-node --configuration production",
    "build:all:verbose": "npx nx build duality-social-lib --verbose --configuration production && npx nx build duality-social-angular --verbose --configuration production && npx nx build duality-social-node --verbose --configuration production",
    "build:all:dev": "npx nx build duality-social-lib --configuration development && npx nx build duality-social-angular --configuration development && npx nx build duality-social-node --configuration development",
    "build:node": "npx nx build duality-social-node --configuration production",
    "build:node:dev": "npx nx build duality-social-node --configuration development",
    "test:all": "npx nx test duality-social-lib && npx nx test duality-social-angular && npx nx test duality-social-node",
    "build": "yarn build:all",
    "serve": "npx nx serve duality-social-node --configuration production",
    "serve:dev": "npx nx serve duality-social-node --configuration development",
    "serve:angular:dev": "npx nx serve duality-social-angular --configuration development",
    "serve:angular": "npx nx serve duality-social-angular --configuration production",
    "npm-globals": "npm install -g npm yarn @angular/cli @nrwl/cli npx nx jest",
    "azure:allow-telemetry": "az config set core.allow_broker=true && az account clear && az login",
    "azure:auth-cycle": "az account clear && az login",
    "azure:clear": "az account clear",
    "azure:login": "az login",
    "clean:caches": "rm -rf ~/.cache && yarn cache clean",
    "clean:locks": "rm -f yarn.lock apps/duality-social-angular/yarn.lock apps/duality-social-node/yarn.lock libs/duality-social-lib/yarn.lock",
    "clean:modules": "rm -rf node_modules apps/duality-social-angular/node_modules apps/duality-social-node/node_modules libs/duality-social-lib/node_modules",
    "clean:output": "rm -rf dist/",
    "docker:build:direct": "docker build -f Dockerfile -t duality-social-app-1 .",
    "docker:build": "docker-compose build --no-cache",
    "docker:dev": "docker-compose -f .devcontainer/docker-compose.yml",
    "docker:down": "docker-compose down",
    "docker:keycloak": "docker-compose -f docker-compose.keycloak.yml",
    "docker:keycloak:build": "docker-compose -f docker-compose.keycloak.yml build --no-cache",
    "docker:keycloak:down": "docker-compose -f docker-compose.keycloak.yml down",
    "docker:keycloak:up": "docker-compose -f docker-compose.keycloak.yml up",
    "docker:keycloak:logs": "docker-compose -f docker-compose.keycloak.yml logs",
    "docker:ps": "docker-compose -f .devcontainer/docker-compose.yml ps",
    "docker:ps-dev": "docker-compose -f .devcontainer/docker-compose.yml -p duality-social_devcontainer ps",
    "docker:logs": "docker compose logs",
    "docker:logs-dev": "docker-compose -f .devcontainer/docker-compose.yml -p duality-social_devcontainer logs",
    "docker:up": "docker-compose up -d",
    "git:push:main:all": "git push origin main && git push upstream main && git push keybase main",
    "migrate:latest": "npx nx migrate latest",
    "migrate:run-migrations": "npx nx migrate --run-migrations"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.2.0",
    "@angular/common": "~15.2.0",
    "@angular/compiler": "~15.2.0",
    "@angular/core": "~15.2.0",
    "@angular/forms": "~15.2.0",
    "@angular/platform-browser": "~15.2.0",
    "@angular/platform-browser-dynamic": "~15.2.0",
    "@angular/router": "~15.2.0",
    "@docusaurus/core": "2.1.0",
    "@docusaurus/preset-classic": "2.1.0",
    "@mdx-js/react": "^1.6.22",
    "@nrwl/angular": "16.4.2",
    "@types/express": "~4.17.13",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "bson": "^5.2.0",
    "clsx": "^1.2.1",
    "express": "~4.18.1",
    "mongoose": "^7.0.4",
    "prism-react-renderer": "^1.3.5",
    "pusher": "^5.1.2",
    "pusher-js": "^8.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.0",
    "@angular-devkit/core": "~15.2.0",
    "@angular-devkit/schematics": "~15.2.0",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "~15.2.0",
    "@angular/language-service": "~15.2.0",
    "@docusaurus/module-type-aliases": "2.1.0",
    "@nrwl/cypress": "16.4.2",
    "@nrwl/esbuild": "16.4.2",
    "@nrwl/eslint-plugin-nx": "16.4.2",
    "@nrwl/jest": "16.4.2",
    "@nrwl/js": "16.4.2",
    "@nrwl/linter": "16.4.2",
    "@nrwl/node": "16.4.2",
    "@nrwl/nx-cloud": "16.0.5",
    "@nrwl/web": "16.4.2",
    "@nrwl/workspace": "16.4.2",
    "@nx-plus/docusaurus": "^15.0.0-rc.0",
    "@schematics/angular": "~15.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "~4.17.13",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^12.2.0",
    "esbuild": "^0.17.5",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jest-preset-angular": "~13.0.0",
    "nx": "16.4.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  }
}

